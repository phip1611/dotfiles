# Common Plugins for "oh my zsh". This list only includes plugins shipped with oh-my-zsh.
# All other zsh-plugins are managed with zplug.
common_zsh_plugins=(
  git
  yarn
  npm
  ng
  colored-man-pages
  httpie
  mvn
  urltools
  colorize

  # rustc, cargo, rustup
  rust

  ripgrep

  docker
  docker-compose
)
